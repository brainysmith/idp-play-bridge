        targetNamespace="urn:identityblitz:shibboleth:2.0:resolver:ar" elementFormDefault="qualified">

    <import namespace="urn:mace:shibboleth:2.0:resolver" schemaLocation="classpath:/schema/shibboleth-2.0-attribute-resolver.xsd"/>

    <complexType name="SessionConnector">

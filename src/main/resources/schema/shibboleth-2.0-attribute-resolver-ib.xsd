            <documentation>
                A data connector that gets its information from the current Identity Blitz principal stored in the session.
            </documentation>
        </annotation>
        <complexContent>
            <extension base="resolver:BaseDataConnectorType">
            </extension>
        </complexContent>
    </complexType>

</schema>
